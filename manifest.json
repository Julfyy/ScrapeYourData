{
  "manifest_version": 2,

  "name": "ScrapeYourData",
  "description": "This extension can scrape and analyze data from different sites",
  "version": "1.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"]
}
